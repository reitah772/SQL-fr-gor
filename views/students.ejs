<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Students List</title>
</head>
<body>
    <h1>Students</h1>
    <ul>
        <% students.forEach(function(student) { %>
            <li><%= student.firstName %> <%= student.lastName %> - <%= student.email %></li>
        <% }); %>
    </ul>
    <a href="/">Back to Home</a>
</body>
</html>
